@model IEnumerable<WebApplication5.Models.Customer>
@{
}
@if (Model.Count() <= 0)
{
    <p class="text-center text-danger">Customer was not found on this location!</p>
}
else
{
<table class="table table-bordered">
    <tr>
        <th>Name </th>
        <th>Email</th>
        <th>Mobile</th>
    </tr>
    @foreach (var cust in Model)
    {
        <tr>
            <td>@cust.Name</td>
            <td>@cust.Email</td>
            <td>@cust.Mobile</td>
        </tr>
    }
    
</table>
}
